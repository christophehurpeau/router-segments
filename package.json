{
  "name": "router-segments",
  "version": "1.1.0",
  "description": "Router with segments",
  "keywords": [
    "router",
    "segments"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "MIT",
  "repository": "git@github.com:christophehurpeau/router-segments.git",
  "homepage": "https://github.com/christophehurpeau/router-segments",
  "bugs": {
    "url": "https://github.com/christophehurpeau/router-segments/issues"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "main": "lib/index.js",
  "module": "./dist/index-browser.es.js",
  "module-dev": "./dist/index-browser-dev.es.js",
  "browser": "./dist/index.cjs.js",
  "browser-dev": "./dist/index-dev.cjs.js",
  "module:node": "./dist/index-node8.es.js",
  "module:node-dev": "./dist/index-node8-dev.es.js",
  "module:browser": "./dist/index-browser.es.js",
  "module:browser-dev": "./dist/index-browser-dev.es.js",
  "module:modern-browsers": "./dist/index-browsermodern.es.js",
  "module:modern-browsers-dev": "./dist/index-browsermodern-dev.es.js",
  "scripts": {
    "build": "pob-build",
    "clean": "rm -Rf docs dist",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "flow": "flow",
    "generate:api": "pob-build-doc",
    "generate:docs": "rm -Rf docs ; yarn run generate:api && yarn run generate:test-coverage",
    "generate:test-coverage": "rm -Rf docs/coverage/ ; NODE_ENV=production BABEL_ENV=test jest --coverage --coverageReporters=lcov --coverageDirectory=docs/coverage/",
    "lint": "eslint src/",
    "postcheckout": "yarnhook",
    "postmerge": "yarnhook",
    "postrewrite": "yarnhook",
    "precommit": "lint-staged",
    "preversion": "yarn run lint && yarn run build && yarn run generate:docs && pob-repository-check-clean",
    "release": "pob-repository-check-clean && pob-release",
    "test": "jest",
    "version": "pob-version",
    "watch": "pob-watch"
  },
  "lint-staged": {
    "{package.json,.eslintrc.json}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.json": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.js": [
      "eslint --fix --quiet",
      "git add"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 100
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "jest": {
    "cacheDirectory": "./node_modules/.cache/jest",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js",
      "<rootDir>/src/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "dependencies": {
    "flow-runtime": "^0.17.0",
    "nightingale-logger": "^4.0.0",
    "path-to-regexp": "^2.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.1.3",
    "@commitlint/config-conventional": "^6.1.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-latest-node": "^1.0.0",
    "babel-preset-modern-browsers": "^10.0.1",
    "eslint": "^4.19.1",
    "eslint-config-pob": "^18.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-prettier": "^2.6.0",
    "flow-bin": "^0.69.0",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "jest-junit-reporter": "^1.1.0",
    "jsdoc": "^3.5.5",
    "lint-staged": "^7.0.0",
    "minami": "^1.2.3",
    "pob-babel": "^20.2.0",
    "pob-release": "^3.1.0",
    "prettier": "^1.11.1",
    "yarnhook": "^0.2.0"
  }
}
