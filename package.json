{
  "name": "router-segments",
  "version": "3.0.0",
  "description": "Router with segments",
  "keywords": [
    "router",
    "segments"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "MIT",
  "repository": "https://github.com:christophehurpeau/router-segments.git",
  "homepage": "https://christophehurpeau.github.io/router-segments/",
  "bugs": {
    "url": "https://github.com/christophehurpeau/router-segments/issues"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "browserslist": [
    "> 0.1%",
    "Firefox ESR",
    "not ie < 9",
    "not dead"
  ],
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index-browser.es.js",
  "module-dev": "./dist/index-browser-dev.es.js",
  "browser": "./dist/index-browser.cjs.js",
  "browser-dev": "./dist/index-browser-dev.cjs.js",
  "module:node": "./dist/index-node10.es.js",
  "module:node-dev": "./dist/index-node10-dev.es.js",
  "module:browser": "./dist/index-browser.es.js",
  "module:browser-dev": "./dist/index-browser-dev.es.js",
  "module:modern-browsers": "./dist/index-browsermodern.es.js",
  "module:modern-browsers-dev": "./dist/index-browsermodern-dev.es.js",
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf dist",
    "generate:api": "typedoc --tsconfig tsconfig.doc.json",
    "generate:docs": "rm -Rf docs ; yarn run generate:api ; touch docs/.nojekyll && yarn run generate:test-coverage",
    "generate:test-coverage": "rm -Rf docs/coverage/ ; NODE_ENV=production BABEL_ENV=test jest --coverage --coverageReporters=pob-lcov-reporter --coverageDirectory=docs/coverage/",
    "lint": "tsc && eslint --ext .js,.ts --quiet *.js src",
    "postbuild": "tsc -p tsconfig.build.json",
    "preversion": "yarn run lint && yarn run build && yarn run generate:docs && repository-check-dirty",
    "release": "repository-check-dirty && pob-release",
    "test": "jest",
    "test:watch": "jest",
    "version": "pob-version",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "commitlint": {
    "extends": [
      "@pob/commitlint-config"
    ]
  },
  "jest": {
    "cacheDirectory": "./node_modules/.cache/jest",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts",
      "<rootDir>/src/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "babel-jest"
    },
    "testEnvironment": "node"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "10",
        "formats": [
          "cjs",
          "es"
        ]
      },
      {
        "target": "browser",
        "version": "modern",
        "formats": [
          "es"
        ]
      },
      {
        "target": "browser",
        "formats": [
          "cjs",
          "es"
        ]
      }
    ],
    "entries": [
      "index"
    ]
  },
  "dependencies": {
    "nightingale-logger": "^10.0.1",
    "path-to-regexp": "^1.7.0"
  },
  "devDependencies": {
    "@babel/core": "7.10.1",
    "@babel/preset-env": "7.10.1",
    "@pob/commitlint-config": "1.1.2",
    "@pob/eslint-config": "37.2.0",
    "@pob/eslint-config-node": "37.2.0",
    "@pob/eslint-config-typescript": "37.2.0",
    "@pob/root": "2.2.1",
    "@types/jest": "25.2.3",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "babel-jest": "26.0.1",
    "babel-preset-latest-node": "4.1.0",
    "babel-preset-modern-browsers": "14.2.1",
    "eslint": "6.8.0",
    "eslint-import-resolver-node": "0.3.3",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-unicorn": "18.0.1",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "jest-junit-reporter": "1.1.0",
    "pob-babel": "24.12.0",
    "pob-lcov-reporter": "2.3.0",
    "pob-release": "5.6.0",
    "prettier": "2.0.5",
    "typedoc": "0.17.7",
    "typescript": "3.9.3"
  }
}
